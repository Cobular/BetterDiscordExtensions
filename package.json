{
  "name": "copymoji",
  "version": "0.0.1",
  "description": "A BetterDiscord plugin to enable the copying of actual emojis, not their names",
  "repository": {
    "type": "git",
    "url": ""
  },
  "devDependencies": {
    "@babel/core": "^7.10.5",
    "@babel/preset-env": "^7.10.4",
    "@babel/register": "^7.10.5",
    "eslint": "^7.14.0",
    "eslint-plugin-react": "^7.21.5",
    "mocha": "^8.1.3",
    "raw-loader": "^4.0.1",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12"
  },
  "scripts": {
    "build_plugin": "node ./scripts/build.js local",
    "build_plugin_packed": "node ./scripts/pack.js development",
    "build": "node ./scripts/pack.js development 0PluginLibrary",
    "build-prod": "node ./scripts/pack.js production 0PluginLibrary",
    "webpack": "webpack",
    "lint": "eslint --ext .js ./plugins/ --ignore-pattern '*.json'"
  },
  "author": "Cobular",
  "license": "MIT",
  "eslintIgnore": [
  ],
  "defaultConfig": {
    "pluginsFolder": "./plugins",
    "releaseFolder": "./release",
    "copyToBD": true,
    "addInstallScript": true
  }
}
